    "@xmlns:com": "urn:nl/procura/gba/v1.5/diensten/complextypes",
    "@xmlns:geh": "urn:nl/procura/gba/v1.5/diensten/geheimhouding",
    "soapenv:Body.dien:AanvraagRequest.dien:GeheimhoudingaanvraagRequest.geh:Zaakgegevens.com:ZaakID": "identificatie",
    "soapenv:Body.dien:AanvraagRequest.dien:GeheimhoudingaanvraagRequest.geh:Zaakgegevens.com:Leverancier": "opdrachtgevendeOrganisatie",
