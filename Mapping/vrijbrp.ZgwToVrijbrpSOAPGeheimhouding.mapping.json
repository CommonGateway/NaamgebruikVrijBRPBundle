{
  "title": "ZgwToVrijbrpSOAPGeheimhouding",
  "$id": "https://vrijbrp.nl/mapping/vrijbrp.ZgwToVrijbrpSOAPGeheimhouding.mapping.json",
  "$schema": "https://json-schema.org/draft/2020-12/mapping",
  "version": "0.0.4",
  "passTrough": false,
  "mapping": {
